<div class="container">
  <article class="">
    <ul>
      <li class="col-md-2">Code: <%= @notebook.name %></li>
      <% if current_user %>
        <li class="col-md-2"><a href="<%= notebook_path(@notebook.id) %>" data-method="delete" class="btn btn-danger"> Delete </a></li>
      <% end %>
    </ul>
  </article>
</div>
<section class="row text-center cities-controls">
    <a href="<%= notebooks_path %>" class="btn btn-default"> Return my Notebook's </a>
</section>
